<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.innovation.platform.upms.mapper.TbApiInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.innovation.platform.upms.common.model.TbApiInfo">
        <id column="id" property="id" />
        <result column="api_code" property="apiCode" />
        <result column="api_name" property="apiName" />
        <result column="api_sort" property="apiSort" />
        <result column="channel_type" property="channelType" />
        <result column="status" property="status" />
        <result column="contacts" property="contacts" />
        <result column="company" property="company" />
        <result column="mobile" property="mobile" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, api_code AS apiCode, api_name AS apiName, api_sort AS apiSort, channel_type AS channelType, status, contacts, company, mobile, create_time AS createTime, update_time AS updateTime
    </sql>

</mapper>
